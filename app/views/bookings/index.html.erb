<h2>My bookings</h2>

<div class="container">
  <div class="row">
    <% @bookings.each do |booking| %>
      <div class="col-sm-12">
        <div class="card">
          <div class="card-image"><img src="https://unsplash.it/200*200" alt="#"></div>
          <p>name:<%= booking.lense.name %></p>
          <p>Description of the lens:<%= booking.lense.description %></p>
          <p>start_date:<%= booking.start_date %></p>
          <p>end_date:<%= booking.end_date %></p>
        </div>
      </div>
    <% end %>
  </div>
</div>
